<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>CB.System</RootNamespace>
    <Configurations>Debug;Release</Configurations>
    <Description>CB.System is a collection of common classes to solve frequently occurring problems.</Description>
    <Platforms>x64;AnyCPU</Platforms>
    <AssemblyName>CB.System</AssemblyName>
    <Version>0.1.0</Version>
    <PackageProjectUrl>https://github.com/Borck/CB-Libs</PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <PackageIcon>icon.png</PackageIcon>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2019.1.1" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\images\icon.png" Pack="true" PackagePath="\">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
